package main;

public class Car extends Vehicle {
	
	private String size;
	
	public Car(String size) {
		super(4, 5, "Land");
	}
	
	public void move() {
		System.out.println("The Car moves forward.");
	}
	
	public void start() {
		System.out.println("The Car has started.");
		setEngineOn(true);
	}

	@Override
	public String toString() {
		return "Car [size=" + size + "]";
	}
	

}
