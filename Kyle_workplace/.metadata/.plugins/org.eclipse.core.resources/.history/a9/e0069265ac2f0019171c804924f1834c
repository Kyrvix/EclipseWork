		storedVehicles.add(p1);
		storedVehicles.add(m1);
		storedVehicles.forEach(System.out::println);
		
		for(int i = 0; i < storedVehicles.size(); i++) {
			
		}
		System.out.println(c1.getClass().toString());
		
		
	}


