package main;

import java.awt.Window.Type;
import java.util.ArrayList;
import java.util.List;

public class Garage {
	
	private List<Vehicle> vehicles = new ArrayList<>();
	public double calcBill() {
		double sum = 0;
		for (Vehicle v : vehicles) {
			sum += v.calcBill();
		}
		return sum;
	}


